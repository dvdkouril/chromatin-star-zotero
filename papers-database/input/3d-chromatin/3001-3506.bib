@article{ WOS:000085645100009,
Author = {Bourguet, W and Vivat, V and Wurtz, JM and Chambon, P and Gronemeyer, H
   and Moras, D},
Title = {Crystal structure of a heterodimeric complex of RAR and RXR
   ligand-binding domains},
Journal = {MOLECULAR CELL},
Year = {2000},
Volume = {5},
Number = {2},
Pages = {289-298},
Month = {FEB},
Abstract = {The crystal structure of a heterodimer between the ligand-binding
   domains (LBDs) of the human RAR alpha bound to a selective antagonist
   and the constitutively active mouse RXR alpha F318A mutant shows that,
   pushed by a bulky extension of the ligand, RAR alpha helix H12 adopts an
   antagonist position. The unexpected presence of a fatty acid in the
   ligand-binding pocket of RXR alpha F318A is likely to account for its
   apparent ``constitutivity.{''} Specific conformational changes suggest
   the structural basis of pure and partial antagonism. The RAR-RXR
   heterodimer interface is similar to that observed in most nuclear
   receptor (NR) homodimers. A correlative analysis of 3D structures and
   sequences provides a novel view on dimerization among members of the
   nuclear receptor superfamily.},
