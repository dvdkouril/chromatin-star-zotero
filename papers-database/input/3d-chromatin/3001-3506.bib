@article{ WOS:000282473700037,
Author = {Kabesch, M. and Michel, S. and Tost, J.},
Title = {Epigenetic mechanisms and the relationship to childhood asthma},
Journal = {EUROPEAN RESPIRATORY JOURNAL},
Year = {2010},
Volume = {36},
Number = {4},
Pages = {950-961},
Month = {OCT},
Abstract = {Epigenetic mechanisms mediate genomic adaption to the environment and
   epigenetic alterations can contribute to the development of disease
   phenotypes, as can genetic variants. A variety of molecular mechanisms
   are involved in epigenetic regulation, including post-transcriptional
   histone modifications, histone variants, ATP-dependent chromatin
   remodelling complexes, polycomb/trithorax protein complexes, small and
   other noncoding RNAs (small interfering RNA and micro RNAs) and DNA
   methylation. Epigenetic mechanisms have been identified in cancer but
   very little is known about these effects in complex diseases such as
   asthma. Epigenetic mechanisms have been found to play a primordial role
   in T-cell differentiation and novel aspects of asthma and allergy
   development are now investigated by systematic epigenetic studies. Here
   we give an introduction to epigenetics, review the existing literature
   in relation to asthma and asthma-related mechanisms and hypothesise on
   feasible approaches for the study of epigenetics in asthma in the
   future.},
Publisher = {EUROPEAN RESPIRATORY SOC JOURNALS LTD},
Address = {442 GLOSSOP RD, SHEFFIELD S10 2PX, ENGLAND},
Type = {Article},
Language = {English},
