@article{ WOS:001221062500001,
Author = {Keller, Debora and Stinus, Sonia and Umlauf, David and Gourbeyre, Edith
   and Biot, Eric and Olivier, Nicolas and Mahou, Pierre and Beaurepaire,
   Emmanuel and Andrey, Philippe and Crabbe, Laure},
Title = {Non-random spatial organization of telomeres varies during the cell
   cycle and requires LAP2 and BAF},
Journal = {ISCIENCE},
Year = {2024},
Volume = {27},
Number = {4},
Month = {APR 19},
Abstract = {Spatial genome organization within the nucleus influences major
   biological processes and is impacted by the configuration of linear
   chromosomes. Here, we applied 3D spatial statistics and modeling on high
   -resolution telomere and centromere 3D -structured illumination
   microscopy images in cancer cells. We found a multi -scale organization
   of telomeres that dynamically evolved from a mixed clustered -and
   -regular distribution in early G1 to a purely regular distribution as
   cells progressed through the cell cycle. In parallel, our analysis
   revealed two pools of peripheral and internal telomeres, the proportions
   of which were inverted during the cell cycle. We then conducted a
   targeted screen using MadID to identify the molecular pathways driving
   or maintaining telomere anchoring to the nuclear envelope observed in
   early G1. Lamina -associated polypeptide (LAP) proteins were found
   transiently localized to telomeres in anaphase, a stage where LAP2 a
   initiates the reformation of the nuclear envelope, and impacted telomere
   redistribution in the next interphase together with their partner
   barrier-to-autointegration factor (BAF).},
