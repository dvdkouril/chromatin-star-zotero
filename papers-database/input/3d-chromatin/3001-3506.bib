@article{ WOS:000659463100004,
Author = {Aboelnour, Erin and Bonev, Boyan},
Title = {Decoding the organization, dynamics, and function of the 4D genome},
Journal = {DEVELOPMENTAL CELL},
Year = {2021},
Volume = {56},
Number = {11},
Pages = {1562-1573},
Month = {JUN 7},
Abstract = {Understanding how complex cell-fate decisions emerge at the molecular
   level is a key challenge in developmental biology. Despite remarkable
   progress in decoding the contribution of the linear epigenome, how
   spatial genome architecture functionally informs changes in gene
   expression remains unclear. In this review, we discuss recent insights
   in elucidating the molecular landscape of genome folding, emphasizing
   the multi-layered nature of the 3D genome, its importance for gene
   regulation, and its spatiotemporal dynamics. Finally, we discuss how
   these new concepts and emergent technologies will enable us to address
   some of the outstanding questions in development and disease.},
