
@article{ WOS:000187469900003,
Author = {Fankhauser, G},
Title = {The development of fragments of the fertilized triton egg with the egg
   nucleus alone ('gyno-merogony')},
Journal = {JOURNAL OF EXPERIMENTAL ZOOLOGY},
Year = {1937},
Volume = {75},
Number = {3},
Pages = {413-469},
Month = {APR},
Abstract = {Eggs of the European newt, Triton palmatus, were constricted by means of
   a hairloop (Spe-mann''s method) from 14 to 60 min. after insemination.
   In 154 expts., the constriction isolated the 2d maturation spindle in
   one-half of the egg, while all the spermatozoa of the normally
   polyspermic egg were confined to the other half. Of the 105 fragments
   containing the maturation spindle which were allowed to develop, 77\%
   segmented or made at least an attempt at cleavage. The development came
   to an end at stages ranging from early cleavage to late blastula. The
   cytological study shows that, during the first 3 hrs. after
   insemination, the behavior of the egg chromatin is practically the same
   as in normal fertilization. Maturation is completed, and the egg nucleus
   moves toward the cytoplasmic center of the fragment. No aster is formed.
   After the 3d hr., the egg nucleus exhibits a variety of different
   reactions 1) it degenerates without an attempt at division, or 2) grows
   to a very large vesicle and forms more or less normal chromosomes; 3) it
   divides by a peculiar type of anastral mitosis, which is followed by
   cell division and may be repeated several times, or 4) forms an abortive
   monaster, or 5) divides by normal mitosis. The detailed study of these
   various reactions of the egg nucleus affords interesting results
   concerning different problems of fertilization, mitosis, and cell
   division.},
