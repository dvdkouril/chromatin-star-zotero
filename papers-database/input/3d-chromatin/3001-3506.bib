Editor = {Solomonides, T and Blanquer, I and Breton, V and Glatard, T and Legre, Y},
Title = {Parallel High-Performance Grid Computing: Capabilities and Opportunities
   of a Novel Demanding Service and Business Class Allowing Highest
   Resource Efficiency},
Booktitle = {HEALTHGRID APPLICATIONS AND CORE TECHNOLOGIES},
