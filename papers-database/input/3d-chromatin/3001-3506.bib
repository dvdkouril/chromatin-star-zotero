Language = {English},
Affiliation = {Xuan, JS (Corresponding Author), Univ Sci \& Technol Beijing, Dept Biol Sci \& Technol, 30 Xueyuan Rd, Beijing 100083, Peoples R China.
   Xuan, Jinsong, Univ Sci \& Technol Beijing, Dept Biol Sci \& Technol, Beijing 100083, Peoples R China.
   Yao, Hongwei; Feng, Yingang; Wang, Jinfeng, Chinese Acad Sci, Inst Biophys, Nat Lab Biomacromol, Beijing 100101, Peoples R China.},
DOI = {10.1016/j.pep.2008.11.003},
ISSN = {1046-5928},
EISSN = {1096-0279},
