@article{ WOS:000685608800009,
Author = {Sharif, Ghada M. and Campbell, Moray J. and Nasir, Apsra and Sengupta,
   Surojeet and Graham, Garrett T. and Kushner, Max H. and Kietzman,
   William B. and Schmidt, Marcel O. and Pearson, Gray W. and Loudig,
   Olivier and Fineberg, Susan and Wellstein, Anton and Riegel, Anna T.},
Title = {An AIB1 Isoform Alters Enhancer Access and Enables Progression of
   Early-Stage Triple-Negative Breast Cancer},
Journal = {CANCER RESEARCH},
Year = {2021},
Volume = {81},
Number = {16},
Pages = {4230-4241},
Month = {AUG},
Abstract = {AIB1 Delta 4 is an N-terminally truncated isoform of the oncogene
   amplified in breast cancer 1 (AIB1) with increased expression in
   high-grade human ductal carcinoma in situ (DCIS). However, the role of
   AIB1 Delta 4 in DCIS malignant progression has not been defined. Here we
   CRISPR-engineered RNA splice junctions to produce normal and early-stage
   DCIS breast epithelial cells that expressed only AIB1 Delta 4. These
   cells showed enhanced motility and invasion in 3D cell culture. In
   zebrafish, AIB1 Delta 4-expressing cells enabled invasion of parental
   cells when present in a mixed population. In mouse xenografts, a
   subpopulation of AIB1 Delta 4 cells mixed with parental cells enhanced
   tumor growth, recurrence, and lung metastasis. AIB1 Delta 4 chromatin
   immunoprecipitation sequencing revealed enhanced binding to regions
   including peroxisome proliferator-activated receptor (PPAR) and
   glucocorticoid receptor (GR) genomic recognition sites. H3K27ac and
   H3K4me1 genomic engagement patterns revealed selective activation of
   breast cancer-specific enhancer sites by AIB1 Delta 4. AIB1 Delta 4
   cells displayed upregulated inflammatory response genes and
   downregulated PPAR signaling gene expression patterns. In the presence
   of AIB1 Delta 4 enabler cells, parental cells increased NF-kappa B and
   WNT signaling. Cellular crosstalk was inhibited by the PPAR gamma
   agonist efatutazone but was enhanced by treatment with the GR agonist
   dexamethasone. In conclusion, expression of the AIB1 Delta 4-selective
   cistrome in a small subpopulation of cells triggers an ``enabler{''}
   phenotype hallmarked by an invasive transcriptional program and
   collective malignant progression in a heterogeneous tumor population.
   Significance: A minor subset of early-stage breast cancer cells
   expressing AIB1 Delta 4 enables bulk tumor cells to become invasive,
   suggesting that selective eradication of this population could impair
   breast cancer metastasis.
