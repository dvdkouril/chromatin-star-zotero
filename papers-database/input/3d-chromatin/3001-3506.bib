@article{ WOS:000793456800023,
Author = {Dong, Chen and Fu, Shuhua and Karvas, Rowan M. and Chew, Brian and
   Fischer, Laura A. and Xing, Xiaoyun and Harrison, Jessica K. and Popli,
   Pooja and Kommagani, Ramakrishna and Wang, Ting and Zhang, Bo and
   Theunissen, Thorold W.},
Title = {A genome-wide CRISPR-Cas9 knockout screen identifies essential and
   growth-restricting genes in human trophoblast stem cells},
Journal = {NATURE COMMUNICATIONS},
Year = {2022},
Volume = {13},
Number = {1},
Month = {MAY 10},
Abstract = {The recent derivation of human trophoblast stem cells (hTSCs) provides a
   scalable in vitro model system of human placental development, but the
   molecular regulators of hTSC identity have not been systematically
   explored thus far. Here, we utilize a genome-wide CRISPR-Cas9 knockout
   screen to comprehensively identify essential and growth-restricting
   genes in hTSCs. By cross-referencing our data to those from similar
   genetic screens performed in other cell types, as well as gene
   expression data from early human embryos, we define hTSC-specific and
   -enriched regulators. These include both well-established and previously
   uncharacterized trophoblast regulators, such as ARID3A, GATA2, and TEAD1
   (essential), and GCM1, PTPN14, and TET2 (growth-restricting). Integrated
   analysis of chromatin accessibility, gene expression, and genome-wide
   location data reveals that the transcription factor TEAD1 regulates the
   expression of many trophoblast regulators in hTSCs. In the absence of
   TEAD1, hTSCs fail to complete faithful differentiation into extravillous
   trophoblast (EVT) cells and instead show a bias towards
   syncytiotrophoblast (STB) differentiation, thus indicating that this
   transcription factor safeguards the bipotent lineage potential of hTSCs.
   Overall, our study provides a valuable resource for dissecting the
   molecular regulation of human placental development and diseases.
   Here the authors perform a genome-wide CRISPR-Cas9 knockout screen to
   systematically identify and characterize essential and
   growth-restricting genes in human trophoblast cells. They identify TEAD1
   as a key regulator that plays an important role in the specification,
   maintenance, and differentiation of the human trophoblast lineage by
   modulating chromatin architecture and gene expression.},
