@article{ WOS:000982581400005,
Author = {Contessoto, Vinicius G. and Dudchenko, Olga and Aiden, Erez Lieberman
   and Wolynes, Peter G. and Onuchic, Jose N. and Di Pierro, Michele},
Title = {Interphase chromosomes of the <i>Aedes aegypti</i> mosquito are liquid
   crystalline and can sense mechanical cues},
Journal = {NATURE COMMUNICATIONS},
Year = {2023},
Volume = {14},
Number = {1},
Month = {JAN 19},
Abstract = {We use data-driven physical simulations to study the three-dimensional
   architecture of the Aedes aegypti genome. Hi-C maps exhibit both a broad
   diagonal and compartmentalization with telomeres and centromeres
   clustering together. Physical modeling reveals that these observations
   correspond to an ensemble of 3D chromosomal structures that are folded
   over and partially condensed. Clustering of the centromeres and
   telomeres near the nuclear lamina appears to be a necessary condition
   for the formation of the observed structures. Further analysis of the
   mechanical properties of the genome reveals that the chromosomes of
   Aedes aegypti, by virtue of their atypical structural organization, are
   highly sensitive to the deformation of the nuclei. This last finding
   provides a possible physical mechanism linking mechanical cues to gene
   regulation.},
