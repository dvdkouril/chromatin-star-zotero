@article{ WOS:001113452500010,
Author = {Sen Gupta, Ayantika and Seidel, Chris and Tsuchiya, Dai and Mckinney,
   Sean and Yu, Zulin and Smith, Sarah E. and Unruh, Jay R. and Gerton,
   Jennifer L.},
Title = {Defining a core configuration for human centromeres during mitosis},
Journal = {NATURE COMMUNICATIONS},
Year = {2023},
Volume = {14},
Number = {1},
Month = {DEC 1},
Abstract = {The centromere components cohesin, CENP-A, and centromeric DNA are
   essential for biorientation of sister chromatids on the mitotic spindle
   and accurate sister chromatid segregation. Insight into the 3D
   organization of centromere components would help resolve how centromeres
   function on the mitotic spindle. We use ChIP-seq and super-resolution
   microscopy with single particle averaging to examine the geometry of
   essential centromeric components on human chromosomes. Both modalities
   suggest cohesin is enriched at pericentromeric DNA. CENP-A localizes to
   a subset of the alpha-satellite DNA, with clusters separated by similar
   to 562 nm and a perpendicular intervening similar to 190 nM wide axis of
   cohesin in metaphase chromosomes. Differently sized alpha-satellite
   arrays achieve a similar core structure. Here we present a working model
   for a common core configuration of essential centromeric components that
   includes CENP-A nucleosomes, alpha-satellite DNA and pericentromeric
   cohesion. This configuration helps reconcile how centromeres function
   and serves as a foundation to add components of the chromosome
   segregation machinery.},
