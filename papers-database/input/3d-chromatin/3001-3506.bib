@article{ WOS:000168979900002,
Author = {Birger, Y and Ito, Y and West, KL and Landsman, D and Bustin, M},
Title = {HMGN4, a newly discovered nucleosome-binding protein encoded by an
   intronless gene},
Journal = {DNA AND CELL BIOLOGY},
Year = {2001},
Volume = {20},
Number = {5},
Pages = {257-264},
Month = {MAY},
Abstract = {We describe a newly discovered nuclear protein, HMGN4, that is closely
   related to the canonical HMGN2 nucleosome-binding protein. The protein
   is encoded by an intronless gene, which, in humans, is located in the
   hereditary hemachromatosis region at position 6p21.3, A single similar
   to2-kb HMGN4 mRNA was found to be expressed, in variable amounts, in all
   human tissues tested; however, the HMGN4 transcript was significantly
   less abundant than that of HMGN2, The HMGN4 protein could be detected in
   HeLa cells by Western analysis with an antibody elicited against a
