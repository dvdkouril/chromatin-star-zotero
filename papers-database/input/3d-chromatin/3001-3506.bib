   Veld, Pim J. Huis and Guo, Chunguang and Yoon, Hye Suk and Denizot, Yves
   and Holwerda, Sjoerd J. B. and de Laat, Wouter and Cogne, Michel and
   Shi, Yang and Alt, Frederick W. and Busslinger, Meinrad},
Title = {Flexible Long-Range Loops in the V<sub>H</sub> Gene Region of the
   <i>Igh</i> Locus Facilitate the Generation of a Diverse Antibody
   Repertoire},
Journal = {IMMUNITY},
