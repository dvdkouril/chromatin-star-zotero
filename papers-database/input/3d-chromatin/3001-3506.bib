@article{ WOS:000462721900040,
Author = {Kantidze, Omar L. and Luzhin, V, Artem and Nizovtseva, V, Ekaterina and
   Safina, Alfiya and Valieva, Maria E. and Golov, Arkadiy K. and Velichko,
   Artem K. and Lyubitelev, V, Alexander and Feofanov, V, Alexey and
   Gurova, V, Katerina and Studitsky, Vasily M. and Razin, V, Sergey},
Title = {The anti-cancer drugs curaxins target spatial genome organization},
Journal = {NATURE COMMUNICATIONS},
Year = {2019},
Volume = {10},
Month = {MAR 29},
Abstract = {Recently we characterized a class of anti-cancer agents (curaxins) that
   disturbs DNA/histone interactions within nucleosomes. Here, using a
   combination of genomic and in vitro approaches, we demonstrate that
   curaxins strongly affect spatial genome organization and compromise
   enhancer-promoter communication, which is necessary for the expression
   of several oncogenes, including MYC. We further show that curaxins
   selectively inhibit enhancer-regulated transcription of chromatinized
   templates in cell-free conditions. Genomic studies also suggest that
   curaxins induce partial depletion of CTCF from its binding sites, which
   contributes to the observed changes in genome topology. Thus, curaxins
   can be classified as epigenetic drugs that target the 3D genome
   organization.},
