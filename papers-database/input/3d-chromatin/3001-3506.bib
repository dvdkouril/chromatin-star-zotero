Title = {SpotLearn: Convolutional Neural Network for Detection of Fluorescence In
   Situ Hybridization ( FISH) Signals in High-Throughput Imaging Approaches},
Booktitle = {CHROMOSOME SEGREGATION AND STRUCTURE, VOL 82, 2017},
Series = {Cold Spring Harbor Symposia on Quantitative Biology},
Year = {2017},
Volume = {82},
